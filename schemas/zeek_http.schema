CREATE TABLE zeek_http
(
        day Date,
        file_src String,
        ts String,
        uid String,
        orig_h String, 
        orig_p UInt16,
        resp_h String,
        resp_p UInt16,
        method String,
        host String,
        uri String,
        referrer Nullable(String),
        user_agent Nullable(String),
        status_code Nullable(String)
) 
ENGINE MergeTree()
PARTITION BY toYYYYMMDD(day) 
ORDER BY (day, host, uri) 
SETTINGS index_granularity=8192
